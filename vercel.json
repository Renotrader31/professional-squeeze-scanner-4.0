{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "POLYGON_API_KEY": "@polygon_api_key",
    "UNUSUAL_WHALES_API_KEY": "@unusual_whales_api_key",
    "ORTEX_API_KEY": "@ortex_api_key"
  }
}